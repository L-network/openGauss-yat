-- @testpoint: 验证ADD PARTITION指定列为其它列，合理报错
drop table if exists mysql_partition_t1;
SQL SUCCESS
create table mysql_partition_t1(c1 int,c2 int)
partition by range(c1) (
  partition p1 start(1) end(10000),
  partition p2 start(10000) end(20000),
  partition p3 start(20000) end(40000)
  );
SQL SUCCESS
alter table mysql_partition_t1 add partition p4 column c2 values less than(50000);
?.*ERROR: syntax error at or near "column"
  Position: 49
alter table mysql_partition_t1 add partition (partition p4 column c2 values less than(50000));
?.*ERROR: syntax error at or near "column"
  Position: 60
drop table mysql_partition_t1;
SQL SUCCESS
