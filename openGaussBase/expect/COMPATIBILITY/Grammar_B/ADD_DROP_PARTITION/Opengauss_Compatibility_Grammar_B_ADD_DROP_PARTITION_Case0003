-- @testpoint: 验证增删分区原语法和MySQL兼容语法对全局临时表执行，合理报错
drop table if exists b_table_t3;
SQL SUCCESS
create global temp table b_table_t3(c1 int,c2 text);
SQL SUCCESS
insert into b_table_t3 values(1,'a'),(2,'b'),(3,'c');
SQL SUCCESS
-- 原语法合理报错
alter table b_table_t3 add partition p1 values less than (1);
?.*ERROR: can not add partition against NON-PARTITIONED table
alter table b_table_t3 add partition p1 values(1);
?.*ERROR: can not add partition against NON-PARTITIONED table
alter table b_table_t3 add partition p1 values(1),add partition p2 values(2);
?.*ERROR: can not add partition against NON-PARTITIONED table
alter table b_table_t3 drop partition p1,drop partition p2;
?.*ERROR: Un-support feature
  Detail: Can not drop partition against NON-PARTITIONED table
-- MySQL语法合理报错
alter table b_table_t3 add partition (partition p1 values less than (1));
?.*ERROR: can not add partition against NON-PARTITIONED table
alter table b_table_t3 add partition (partition p1 values(1));
?.*ERROR: can not add partition against NON-PARTITIONED table
alter table b_table_t3 add partition (partition p1 values(1),partition p2 values(2));
?.*ERROR: can not add partition against NON-PARTITIONED table
alter table b_table_t3 drop partition p1,p2;
?.*ERROR: Un-support feature
  Detail: Can not drop partition against NON-PARTITIONED table
drop table b_table_t3;
SQL SUCCESS
