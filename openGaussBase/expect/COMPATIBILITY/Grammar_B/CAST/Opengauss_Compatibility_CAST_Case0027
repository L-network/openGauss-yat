-- @testpoint: cast用例,部分用例合理报错
-- 数据类型是否有长度限制
select cast('$222222222222222222222222222222222222222222222222222222222222222222222222222222222222222222222222222222222222222222222222222222222222222222222222222222222222222222222222222222222222222222222222222222222222222222222222222222222222222222222222'::money as unsigned);
?.*ERROR: value ".*222222222222222222222222222222222222222222222222222222222222222222222222222222222222222222222222222222222222222222222222222222222222222222222222222222222222222222222222222222222222222222222222222222222222222222222222222222222222222222222222" is out of range for type money
?.*
  Where: referenced column: uint8
select cast('$'::money as unsigned);
?.*ERROR: value ".*" is out of range for type money
?.*
  Where: referenced column: uint8
