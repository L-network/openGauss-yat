select make_set(31,'192.168.100.128/25','b','c','d');
+--------------------------+
| make_set                 |
+--------------------------+
| 192.168.100.128/25,b,c,d |
+--------------------------+
select make_set(8,'a','set hll(-1,-1,8,-1)');
+----------+
| make_set |
+----------+
|          |
+----------+
-- 输入空值
select make_set(null,'','2022-09-03');
+----------+
| make_set |
+----------+
|          |
+----------+
select make_set(0,'a','','non est');
+----------+
| make_set |
+----------+
|          |
+----------+
select make_set(12+1,'','');
+----------+
| make_set |
+----------+
|          |
+----------+
select make_set(8,'','');
+----------+
| make_set |
+----------+
|          |
+----------+
-- 输入null
select make_set(null,'null','2022-09-03');
+----------+
| make_set |
+----------+
|          |
+----------+
select make_set(0,'a','null','non est');
+----------+
| make_set |
+----------+
|          |
+----------+
select make_set(12+1,'null','null');
+----------+
| make_set |
+----------+
| null     |
+----------+
select make_set(3,'null','null');
+-----------+
| make_set  |
+-----------+
| null,null |
+-----------+
