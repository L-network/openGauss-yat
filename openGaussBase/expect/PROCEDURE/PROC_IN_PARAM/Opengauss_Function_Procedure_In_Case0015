--test procedure
--测试仅带入参的存储过程（仅有入参类型为VARCHAR(100)的存储过程）
CREATE OR REPLACE PROCEDURE PROC_IN_PARAM_0015(P1 IN VARCHAR) AS
V1 VARCHAR(100);
BEGIN
SELECT T14 INTO V1 FROM TEST_001 WHERE T15=P1;
raise info 'V1:%',V1;
EXCEPTION
WHEN NO_DATA_FOUND THEN  raise info 'NO_DATA_FOUND';
END;
/
SQL SUCCESS
--调用存储过程
call PROC_IN_PARAM_0015('ab');
+--------------------+
| proc_in_param_0015 |
+--------------------+
|                    |
+--------------------+
drop PROCEDURE PROC_IN_PARAM_0015;
SQL SUCCESS
