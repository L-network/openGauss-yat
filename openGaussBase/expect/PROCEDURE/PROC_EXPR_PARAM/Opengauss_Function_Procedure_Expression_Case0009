-- @testpoint: 表达式做为参数的存储过程测试--函数表达式-FLOOR
--创建存储过程
CREATE OR REPLACE PROCEDURE PROC_EXPR_PARAM_009(P1 DECIMAL)  AS
BEGIN
raise info 'P1=:%',P1;
EXCEPTION
WHEN NO_DATA_FOUND THEN raise info 'NO_DATA_FOUND';
END;
/
SQL SUCCESS
--调用存储过程
DECLARE
V1 REAL :=FLOOR(-123.4);
V2 NUMBER :=FLOOR(15.99999);
V3 DECIMAL :=FLOOR(5);
BEGIN
 PROC_EXPR_PARAM_009(V1);
 PROC_EXPR_PARAM_009(V2);
 PROC_EXPR_PARAM_009(V3);
END;
/
SQL SUCCESS
--清理环境
drop PROCEDURE PROC_EXPR_PARAM_009;
SQL SUCCESS
