-- @testpoint: 通过select insert语句把99999999999999999999999999999999999999999999999999999999999999999999999999999赋值给char类型变量
declare
  v_char char(200);
begin
    select 99999999999999999999999999999999999999999999999999999999999999999999999999999 into v_char from sys_dummy;
    raise info 'result:% ',v_char;
end;
/
SQL SUCCESS
