-- @testpoint: box类型，使用(x1,y1),(x2,y2)方式插入较大坐标值
drop table if exists test_box04;
SQL SUCCESS
create table test_box04 (name box);
SQL SUCCESS
SQL SUCCESS
select * from test_box04;
+---------------------------------+
| name                            |
+---------------------------------+
| (1.0E29,1.0E32),(1.0E29,1.0E32) |
+---------------------------------+
drop table test_box04;
SQL SUCCESS
