-- @testpoint: lseg类型，使用[(x1,y1),(x2,y2)]方式插入较大坐标值
drop table if exists test_lseg04;
SQL SUCCESS
create table test_lseg04 (name lseg);
SQL SUCCESS
SQL SUCCESS
SQL SUCCESS
select * from test_lseg04;
