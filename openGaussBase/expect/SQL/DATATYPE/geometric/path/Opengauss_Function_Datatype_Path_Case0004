-- @testpoint: path类型，使用[(x1,y1),...,(xn,yn)]方式插入较大坐标值
drop table if exists test_path04;
SQL SUCCESS
create table test_path04 (name path);
SQL SUCCESS
SQL SUCCESS
select * from test_path04;
+---------------------------------------------------+
| name                                              |
+---------------------------------------------------+
| [(1.0E29,1.0E32),(1.0E29,1.0E32),(1.0E29,1.0E32)] |
+---------------------------------------------------+
drop table test_path04;
SQL SUCCESS
