-- @testpoint: 插入bool类型
drop table if exists tinyint11;
SQL SUCCESS
create table tinyint11 (name tinyint);
SQL SUCCESS
insert into tinyint11 values (false);
SQL SUCCESS
insert into tinyint11 values (true);
SQL SUCCESS
select * from tinyint11;
+------+
| name |
+------+
| 0    |
| 1    |
+------+
drop table tinyint11;
SQL SUCCESS
