-- @testpoint: 插入左边界范围值
drop table if exists binary_double06;
SQL SUCCESS
create table binary_double06 (name binary_double);
SQL SUCCESS
insert into binary_double06 values (1E-307);
SQL SUCCESS
select * from binary_double06;
+----------+
| name     |
+----------+
| 1.0E-307 |
+----------+
drop table binary_double06;
SQL SUCCESS
