-- @testpoint: 插入字符串类型数值
drop table if exists serial_09;
SQL SUCCESS
create table serial_09 (name serial);
SQL SUCCESS
insert into serial_09 values ('12354563');
SQL SUCCESS
insert into serial_09 values ('-12354563');
SQL SUCCESS
insert into serial_09 values ('1');
SQL SUCCESS
insert into serial_09 values ('-1');
SQL SUCCESS
select * from serial_09;
+-----------+
| name      |
+-----------+
| 12354563  |
| -12354563 |
| 1         |
| -1        |
+-----------+
drop table serial_09;
SQL SUCCESS
