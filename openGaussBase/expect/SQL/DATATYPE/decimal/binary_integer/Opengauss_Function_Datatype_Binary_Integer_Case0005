-- @testpoint: 插入字符串类型数值
drop table if exists binary_integer05;
SQL SUCCESS
create table binary_integer05 (name binary_integer);
SQL SUCCESS
insert into binary_integer05 values ('123456');
SQL SUCCESS
insert into binary_integer05 values ('-123456');
SQL SUCCESS
select * from binary_integer05;
+---------+
| name    |
+---------+
| 123456  |
| -123456 |
+---------+
drop table binary_integer05;
SQL SUCCESS
