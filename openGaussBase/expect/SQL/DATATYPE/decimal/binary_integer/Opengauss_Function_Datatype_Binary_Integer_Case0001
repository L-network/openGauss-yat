-- @testpoint: 插入左边界范围值
drop table if exists binary_integer01;
SQL SUCCESS
create table binary_integer01 (name binary_integer);
SQL SUCCESS
insert into binary_integer01 values (-2147483648 );
SQL SUCCESS
select * from binary_integer01;
+-------------+
| name        |
+-------------+
| -2147483648 |
+-------------+
drop table binary_integer01;
SQL SUCCESS
