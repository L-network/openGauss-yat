-- @testpoint: 插入右边界范围值
drop table if exists binary_integer04;
SQL SUCCESS
create table binary_integer04 (name binary_integer);
SQL SUCCESS
insert into binary_integer04 values (2147483647);
SQL SUCCESS
select * from binary_integer04;
+------------+
| name       |
+------------+
| 2147483647 |
+------------+
drop table binary_integer04;
SQL SUCCESS
