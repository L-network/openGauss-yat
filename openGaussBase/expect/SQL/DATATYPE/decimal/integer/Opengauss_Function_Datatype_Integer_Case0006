-- @testpoint: 插入bool类型
drop table if exists integer06;
SQL SUCCESS
create table integer06 (name integer);
SQL SUCCESS
insert into integer06 values (false);
SQL SUCCESS
insert into integer06 values (true);
SQL SUCCESS
select * from integer06;
+------+
| name |
+------+
| 0    |
| 1    |
+------+
drop table integer06;
SQL SUCCESS
