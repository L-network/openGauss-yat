-- @testpoint: 插入浮点数
drop table if exists smallint03;
SQL SUCCESS
create table smallint03 (name smallint);
SQL SUCCESS
insert into smallint03 values (122.3340);
SQL SUCCESS
insert into smallint03 values (0.000001);
SQL SUCCESS
insert into smallint03 values (-122.3340);
SQL SUCCESS
insert into smallint03 values (-0.000001);
SQL SUCCESS
select * from smallint03;
+------+
| name |
+------+
| 122  |
| 0    |
| -122 |
| 0    |
+------+
drop table smallint03;
SQL SUCCESS
