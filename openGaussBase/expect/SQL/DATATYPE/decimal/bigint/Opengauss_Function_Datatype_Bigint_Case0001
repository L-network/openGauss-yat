-- @testpoint: 插入有效正整数
drop table if exists bigint01;
SQL SUCCESS
create table bigint01 (name bigint);
SQL SUCCESS
insert into bigint01 values (120);
SQL SUCCESS
insert into bigint01 values (99999999);
SQL SUCCESS
insert into bigint01 values (112233445566);
SQL SUCCESS
select * from bigint01;
+--------------+
| name         |
+--------------+
| 120          |
| 99999999     |
| 112233445566 |
+--------------+
drop table bigint01;
SQL SUCCESS
