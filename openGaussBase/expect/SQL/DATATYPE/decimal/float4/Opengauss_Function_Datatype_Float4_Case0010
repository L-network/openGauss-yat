-- @testpoint: 插入空值
drop table if exists float4_10;
SQL SUCCESS
create table float4_10 (id int,name float4);
SQL SUCCESS
insert into float4_10 values (1,null);
SQL SUCCESS
insert into float4_10 values (2,'');
SQL SUCCESS
select * from float4_10;
+----+------+
| id | name |
+----+------+
| 1  |      |
| 2  |      |
+----+------+
drop table if exists float4_10;
SQL SUCCESS
