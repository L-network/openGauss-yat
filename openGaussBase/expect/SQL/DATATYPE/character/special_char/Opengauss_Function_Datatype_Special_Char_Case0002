-- @testpoint: 创建列存表，插入数据
drop table if exists special_char_02;
SQL SUCCESS
CREATE  TABLE special_char_02 (id "char") WITH (orientation=COLUMN, compression=no);
SQL SUCCESS
insert into special_char_02 values ('t');
SQL SUCCESS
select * from special_char_02;
+----+
| id |
+----+
| t  |
+----+
drop table if exists special_char_02;
SQL SUCCESS
