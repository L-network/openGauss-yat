-- @testpoint: 插入正常值，字节长度设定为1
-- @modify at: 2020-11-05
drop table if exists test_char_03;
SQL SUCCESS
create table test_char_03 (name char(1));
SQL SUCCESS
insert into test_char_03 values ('a');
SQL SUCCESS
select * from test_char_03;
