-- @testpoint: 插入正常值，设定字节长度为1
-- @modify at: 2020-11-05
drop table if exists test_character_04;
SQL SUCCESS
create table test_character_04 (name character(1));
SQL SUCCESS
insert into test_character_04 values ('a');
SQL SUCCESS
select * from test_character_04;
