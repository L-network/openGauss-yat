-- @testpoint: 插入正常值
drop table if exists test_nvarchar2_01;
SQL SUCCESS
create table test_nvarchar2_01 (name nvarchar2(20));
SQL SUCCESS
insert into test_nvarchar2_01 values ('abcdefgh');
SQL SUCCESS
select * from test_nvarchar2_01;
+----------+
| name     |
+----------+
| abcdefgh |
+----------+
drop table test_nvarchar2_01;
SQL SUCCESS
