--  @testpoint:clob：to_char (string)：clob值的其他场景：success
select to_char(to_clob(''));
+---------+
| to_char |
+---------+
|         |
+---------+
select to_char(to_clob(' '));
+---------+
| to_char |
+---------+
|         |
+---------+
select to_char(to_clob(null));
+---------+
| to_char |
+---------+
|         |
+---------+
