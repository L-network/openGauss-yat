-- @testpoint: 插入负数测试
-- @modified at: 2020-11-13
drop table if exists test_clob_051;
SQL SUCCESS
create table test_clob_051(c_clob clob);
SQL SUCCESS
insert into test_clob_051 values(-123456);
SQL SUCCESS
select * from test_clob_051;
+---------+
| c_clob  |
+---------+
| -123456 |
+---------+
drop table if exists test_clob_051;
SQL SUCCESS
