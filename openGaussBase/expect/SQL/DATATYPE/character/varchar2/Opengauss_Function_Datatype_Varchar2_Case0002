-- @testpoint: 插入正常范围值
drop table if exists test_varchar2_02;
SQL SUCCESS
create table test_varchar2_02 (name varchar2(20));
SQL SUCCESS
insert into test_varchar2_02 values ('abcdefgh');
SQL SUCCESS
select * from test_varchar2_02;
+----------+
| name     |
+----------+
| abcdefgh |
+----------+
drop table test_varchar2_02;
SQL SUCCESS
