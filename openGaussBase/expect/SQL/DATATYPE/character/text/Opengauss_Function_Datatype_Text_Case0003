-- @testpoint: 创建表，插入负值
drop table if exists test_text_03;
SQL SUCCESS
create table test_text_03(c_text text);
SQL SUCCESS
insert into test_text_03 values(-123456);
SQL SUCCESS
select * from test_text_03;
+---------+
| c_text  |
+---------+
| -123456 |
+---------+
drop table test_text_03;
SQL SUCCESS
