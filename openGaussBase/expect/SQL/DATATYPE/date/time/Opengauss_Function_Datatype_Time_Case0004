-- @testpoint: 输入时间相减
DROP TABLE IF EXISTS test_time04;
SQL SUCCESS
CREATE TABLE test_time04 (name time);
SQL SUCCESS
insert into test_time04 values (time '21:21:17' - time '09:21:17');
SQL SUCCESS
select * from test_time04;
+----------+
| name     |
+----------+
| 12:00:00 |
+----------+
DROP TABLE test_time04;
SQL SUCCESS
