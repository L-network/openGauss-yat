-- @testpoint: 自然对数ln(x)入参为合法值
SELECT LN(1);
+-----+
| ln  |
+-----+
| 0.0 |
+-----+
SELECT LN(3);
+--------------------+
| ln                 |
+--------------------+
+--------------------+
+-------------------+
| ln                |
+-------------------+
+-------------------+
+--------------------+
| ln                 |
+--------------------+
+--------------------+
SELECT LN(3.1415926);
+--------------------+
| ln                 |
+--------------------+
+--------------------+