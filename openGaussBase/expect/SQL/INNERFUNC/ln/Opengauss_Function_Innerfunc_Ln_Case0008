-- @testpoint: 自然对数lnx作为having条件
DROP TABLE IF EXISTS TEST_LN;
SQL SUCCESS
CREATE TABLE TEST_LN(A INT,B INT);
SQL SUCCESS
INSERT INTO TEST_LN VALUES(0,1);
SQL SUCCESS
INSERT INTO TEST_LN VALUES(1,4);
SQL SUCCESS
INSERT INTO TEST_LN VALUES(0.5,2);
SQL SUCCESS
SELECT SUM(A),B FROM TEST_LN GROUP BY B HAVING LN(1)=0 order by b;
+-----+---+
| sum | b |
+-----+---+
| 0   | 1 |
| 1   | 2 |
| 1   | 4 |
+-----+---+
DROP TABLE IF EXISTS TEST_LN;
SQL SUCCESS