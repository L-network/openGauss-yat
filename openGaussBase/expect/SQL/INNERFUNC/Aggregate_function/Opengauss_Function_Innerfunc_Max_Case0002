-- @testpoint: 函数max(expression)，所有输入行中expression的最大值，入参为无效值时，合理报错
drop table if exists table_test;
SQL SUCCESS
create table table_test(id integer,
name varchar(10),
age integer,
height dec(5,2),
hobbies text,
tel bigint,
enrollment_time date
);
SQL SUCCESS
SQL SUCCESS
--当入参有两个参数时
select max(id,tel) from table_test;
?.*ERROR: function max\(integer, bigint\) does not exist.*
  Hint: No function matches the given name and argument types. You might need to add explicit type casts.
?.*Position.*
  Where: referenced column: max
drop table if exists table_test;
SQL SUCCESS
