-- @testpoint: 输入参数为字符串型数值
SELECT sqrt('64') AS RESULT;
+--------+
| result |
+--------+
| 8.0    |
+--------+
SELECT sqrt('12.34') AS RESULT;
+--------------------+
| result             |
+--------------------+
| 3.5128336140500593 |
+--------------------+
SELECT sqrt('0.0') AS RESULT;
+--------+
| result |
+--------+
| 0.0    |
+--------+
SELECT sqrt('2.000000') AS RESULT;
+--------------------+
| result             |
+--------------------+
| 1.4142135623730951 |
+--------------------+
