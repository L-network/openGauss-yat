-- @testpoint: 输入参数为有效值
SELECT sqrt(64) AS RESULT;
+--------+
| result |
+--------+
| 8.0    |
+--------+
SELECT sqrt(2.0000000) AS RESULT;
+-------------------+
| result            |
+-------------------+
| 1.414213562373095 |
+-------------------+
SELECT sqrt(0.0) AS RESULT;
+--------+
| result |
+--------+
| 0E-15  |
+--------+
SELECT sqrt(3.56) AS RESULT;
+-------------------+
| result            |
+-------------------+
| 1.886796226411321 |
+-------------------+
