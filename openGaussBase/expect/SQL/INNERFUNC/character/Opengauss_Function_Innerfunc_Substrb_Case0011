-- @testpoint: 只有起始位参数起始位置大于字符长度
DROP TABLE IF EXISTS type_char;
SQL SUCCESS
CREATE TABLE type_char (string1 int);
SQL SUCCESS
insert into type_char values (1234565);
SQL SUCCESS
SELECT substrb(string1,1) from type_char;
+---------+
| substrb |
+---------+
| 1234565 |
+---------+
DROP TABLE IF EXISTS type_char;
SQL SUCCESS
