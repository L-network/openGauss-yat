-- @testpoint: 参数为时间
DROP TABLE IF EXISTS type_char;
SQL SUCCESS
CREATE TABLE type_char (string1 text);
SQL SUCCESS
insert into type_char values (quote_nullable(2017-1-9));
SQL SUCCESS
SELECT * from type_char;
+---------+
| string1 |
+---------+
| '2007'  |
+---------+
DROP TABLE IF EXISTS type_char;
SQL SUCCESS
