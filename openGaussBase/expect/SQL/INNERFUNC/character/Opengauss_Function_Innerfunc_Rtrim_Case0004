-- @testpoint: 汉字搜索
DROP TABLE IF EXISTS type_char;
SQL SUCCESS
CREATE TABLE type_char (string1 char(100));
SQL SUCCESS
insert into type_char values ('我是一个');
SQL SUCCESS
SELECT rtrim(string1,'一个') from type_char;
+-------+
| rtrim |
+-------+
| 我是    |
+-------+
DROP TABLE IF EXISTS type_char;
SQL SUCCESS
