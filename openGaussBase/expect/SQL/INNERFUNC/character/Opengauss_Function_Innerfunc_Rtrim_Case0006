-- @testpoint: 最后一位为字符
DROP TABLE IF EXISTS type_char;
SQL SUCCESS
CREATE TABLE type_char (string1 char(100));
SQL SUCCESS
insert into type_char values ('sdffxxx1');
SQL SUCCESS
SELECT rtrim(string1,1) from type_char;
+---------+
| rtrim   |
+---------+
| sdffxxx |
+---------+
DROP TABLE IF EXISTS type_char;
SQL SUCCESS
