-- @testpoint: 没有搜索字符
DROP TABLE IF EXISTS type_char;
SQL SUCCESS
CREATE TABLE type_char (stringv char(20));
SQL SUCCESS
insert into type_char values ('stringing');
SQL SUCCESS
SELECT position('' in stringv) from type_char;
+----------+
| position |
+----------+
|          |
+----------+
DROP TABLE IF EXISTS type_char;
SQL SUCCESS
