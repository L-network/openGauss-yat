-- @testpoint: 字符串尾匹配
DROP TABLE IF EXISTS type_char;
SQL SUCCESS
CREATE TABLE type_char (string1 char(100));
SQL SUCCESS
insert into type_char values ('Thomas');
SQL SUCCESS
SELECT substring(string1 from  '...$') from type_char;
+-----------+
| substring |
+-----------+
| mas       |
+-----------+
DROP TABLE IF EXISTS type_char;
SQL SUCCESS
