-- @testpoint: 不能进行相互转换的编码
DROP TABLE IF EXISTS type_char;
SQL SUCCESS
CREATE TABLE type_char (string1 char(100));
SQL SUCCESS
insert into type_char values (convert('some text', 'GBK','LATIN1' ));
SQL SUCCESS
SELECT * from type_char;
+------------------------------------------------------------------------------------------------------+
| string1                                                                                              |
+------------------------------------------------------------------------------------------------------+
+------------------------------------------------------------------------------------------------------+
DROP TABLE IF EXISTS type_char;
SQL SUCCESS
