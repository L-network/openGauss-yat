-- @testpoint: 只有起始位参数起始位置大于字符长度
DROP TABLE IF EXISTS type_char;
SQL SUCCESS
CREATE TABLE type_char (string1 char(100));
SQL SUCCESS
insert into type_char values ('');
SQL SUCCESS
SELECT substrb(string1,1) from type_char;
+---------+
| substrb |
+---------+
|         |
+---------+
DROP TABLE IF EXISTS type_char;
SQL SUCCESS
