-- @testpoint: 字符串= 检索字符
DROP TABLE IF EXISTS type_char;
SQL SUCCESS
CREATE TABLE type_char (string1 char(100));
SQL SUCCESS
insert into type_char values ('source');
SQL SUCCESS
SELECT strpos(string1,'source') from type_char;
+--------+
| strpos |
+--------+
| 1      |
+--------+
DROP TABLE IF EXISTS type_char;
SQL SUCCESS
