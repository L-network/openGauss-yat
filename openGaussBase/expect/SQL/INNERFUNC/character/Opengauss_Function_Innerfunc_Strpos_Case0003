-- @testpoint: 大小写匹配
DROP TABLE IF EXISTS type_char;
SQL SUCCESS
CREATE TABLE type_char (string1 char(100));
SQL SUCCESS
insert into type_char values ('sourcercRC');
SQL SUCCESS
SELECT strpos(string1,'rc') from type_char;
+--------+
| strpos |
+--------+
| 4      |
+--------+
DROP TABLE IF EXISTS type_char;
SQL SUCCESS
