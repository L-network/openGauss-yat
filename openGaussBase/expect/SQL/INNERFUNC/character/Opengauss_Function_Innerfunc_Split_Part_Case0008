-- @testpoint: 中文
DROP TABLE IF EXISTS type_char;
SQL SUCCESS
CREATE TABLE type_char (string1 char(100));
SQL SUCCESS
insert into type_char values ('我是一个');
SQL SUCCESS
SELECT split_part(string1,'是',1) from type_char;
+------------+
| split_part |
+------------+
| 我          |
+------------+
DROP TABLE IF EXISTS type_char;
SQL SUCCESS
