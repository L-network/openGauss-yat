-- @testpoint: 正常字符颠倒
DROP TABLE IF EXISTS type_char;
SQL SUCCESS
CREATE TABLE type_char (string1 text);
SQL SUCCESS
insert into type_char values ('abcdel');
SQL SUCCESS
SELECT reverse(string1) from type_char;
+---------+
| reverse |
+---------+
| ledcba  |
+---------+
DROP TABLE IF EXISTS type_char;
SQL SUCCESS
