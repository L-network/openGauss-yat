-- @testpoint: 参数为字符
select nullif('aa','aa');
+--------+
| nullif |
+--------+
|        |
+--------+
select nullif('aaa','aa');
+--------+
| nullif |
+--------+
| aaa    |
+--------+
select nullif('   aa','aa');
+--------+
| nullif |
+--------+
|    aa  |
+--------+
select nullif('   aa','   aa');
+--------+
| nullif |
+--------+
|        |
+--------+
