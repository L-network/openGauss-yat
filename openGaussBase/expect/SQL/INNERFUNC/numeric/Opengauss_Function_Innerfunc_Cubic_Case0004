-- @testpoint: 数字操作符||/(立方根),负数进行开立方
drop table if exists data_01;
SQL SUCCESS
create table data_01 (clo1 float,clo2 int);
SQL SUCCESS
insert into data_01 values (-0.001, -126);
SQL SUCCESS
select ||/ clo1 from data_01;
+----------+
| ?column? |
+----------+
| -0.1     |
+----------+
SELECT ||/ -8 AS RESULT;
+--------+
| result |
+--------+
| -2.0   |
+--------+
drop table if exists data_01;
SQL SUCCESS
