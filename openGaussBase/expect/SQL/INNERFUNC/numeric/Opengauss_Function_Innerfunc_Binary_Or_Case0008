-- @testpoint: 数字操作符|(二进制OR), 空值进行二进制or
drop table if exists data_01;
SQL SUCCESS
create table data_01 (clo1 int,clo2 SMALLINT);
SQL SUCCESS
select clo1 | clo2 from data_01;
+----------+
| ?column? |
+----------+
+----------+
SELECT null | 10  AS RESULT;
+--------+
| result |
+--------+
|        |
+--------+
drop table if exists data_01;
SQL SUCCESS
