-- @testpoint: 数字操作符!!(前缀阶乘),1或者0的阶乘
drop table if exists data_01;
SQL SUCCESS
create table data_01 (clo1 int,clo2 int);
SQL SUCCESS
insert into data_01 values (0, 1);
SQL SUCCESS
select  !!clo1 from data_01;
+----------+
| ?column? |
+----------+
| 1        |
+----------+
select  !!clo2 from data_01;
+----------+
| ?column? |
+----------+
| 1        |
+----------+
SELECT !!1 AS RESULT;
+--------+
| result |
+--------+
| 1      |
+--------+
drop table if exists data_01;
SQL SUCCESS
