-- @testpoint: cos函数入参为(2k+1)π
select cos(pi());
+------+
| cos  |
+------+
| -1.0 |
+------+
select cos(3*pi());
+------+
| cos  |
+------+
| -1.0 |
+------+
select cos(-1*pi());
+------+
| cos  |
+------+
| -1.0 |
+------+
select cos(5*pi());
+------+
| cos  |
+------+
| -1.0 |
+------+
select cos(-3*pi());
+------+
| cos  |
+------+
| -1.0 |
+------+
select cos(201*pi());
+------+
| cos  |
+------+
| -1.0 |
+------+
select cos(-199*pi());
+------+
| cos  |
+------+
| -1.0 |
+------+
