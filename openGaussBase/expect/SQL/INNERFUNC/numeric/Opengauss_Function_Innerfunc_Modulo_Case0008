-- @testpoint: 数字操作符%(求余),给空值求模
drop table if exists data_01;
SQL SUCCESS
create table data_01 (clo1 BIGINT,clo2 BIGINT);
SQL SUCCESS
select clo1 % 3 from data_01;
+----------+
| ?column? |
+----------+
+----------+
SELECT null % 3 AS RESULT;
+--------+
| result |
+--------+
|        |
+--------+
drop table if exists data_01;
SQL SUCCESS
