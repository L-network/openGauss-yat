-- @testpoint: 数字操作符!!(前缀阶乘),阶乘结果过大溢出，合理报错
drop table if exists data_01;
SQL SUCCESS
create table data_01 (clo1 BIGINT,clo2 BIGINT);
SQL SUCCESS
SQL SUCCESS
select  !!clo1 from data_01;
?.*ERROR: value overflows numeric format
select  !!clo2 from data_01;
?.*ERROR: value overflows numeric format
drop table if exists data_01;
SQL SUCCESS
