-- @testpoint: 数字操作符/(除)，货币类型相除
drop table if exists data_01;
SQL SUCCESS
create table data_01 (clo1 money,clo2 money);
SQL SUCCESS
insert into data_01 values (12.34, 0.01);
SQL SUCCESS
select clo1 / 20 from data_01;
+----------+
| ?column? |
+----------+
| 0.61     |
+----------+
SELECT -92233720367758.06 / 10 AS RESULT;
+---------------------+
| result              |
+---------------------+
| -9223372036775.8060 |
+---------------------+
drop table if exists data_01;
SQL SUCCESS
