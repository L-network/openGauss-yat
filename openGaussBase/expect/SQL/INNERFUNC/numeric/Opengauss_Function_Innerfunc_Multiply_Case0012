-- @testpoint: 数字操作符*(乘),算术表达式作乘数
drop table if exists data_01;
SQL SUCCESS
create table data_01 (clo1 int,clo2 int);
SQL SUCCESS
insert into data_01 values (255, 3);
SQL SUCCESS
select clo1 * (2.5 + clo2+12.555) from data_01;
+----------+
| ?column? |
+----------+
| 4604.025 |
+----------+
SELECT 10 * (2.5  + 3+0.5) AS RESULT;
+--------+
| result |
+--------+
| 60.0   |
+--------+
drop table if exists data_01;
SQL SUCCESS
