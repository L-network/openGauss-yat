-- @testpoint: cos函数入参为2kπ
select cos(0);
+-----+
| cos |
+-----+
| 1.0 |
+-----+
select cos(2*pi());
+-----+
| cos |
+-----+
| 1.0 |
+-----+
select cos(-2*pi());
+-----+
| cos |
+-----+
| 1.0 |
+-----+
select cos(4*pi());
+-----+
| cos |
+-----+
| 1.0 |
+-----+
select cos(-4*pi());
+-----+
| cos |
+-----+
| 1.0 |
+-----+
select cos(598*pi());
+-----+
| cos |
+-----+
| 1.0 |
+-----+
select cos(-598*pi());
+-----+
| cos |
+-----+
| 1.0 |
+-----+
