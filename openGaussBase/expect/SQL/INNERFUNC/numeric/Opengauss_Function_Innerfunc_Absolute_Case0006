-- @testpoint: 数字操作符@(绝对值)，数值0求绝对值
drop table if exists data_01;
SQL SUCCESS
create table data_01 (clo1 BIGINT,clo2 BIGINT);
SQL SUCCESS
insert into data_01 values (0, 2);
SQL SUCCESS
select @clo1 from data_01;
+----------+
| ?column? |
+----------+
| 0        |
+----------+
SELECT @0 AS RESULT;
+--------+
| result |
+--------+
| 0      |
+--------+
drop table if exists data_01;
SQL SUCCESS
