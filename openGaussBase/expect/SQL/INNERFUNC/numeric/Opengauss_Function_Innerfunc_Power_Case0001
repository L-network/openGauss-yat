-- @testpoint: 数字操作符^(幂),正整数的幂
drop table if exists data_01;
SQL SUCCESS
create table data_01 (clo1 int,clo2 SMALLINT);
SQL SUCCESS
insert into data_01 values (2, 5);
SQL SUCCESS
select  clo1^clo2 from data_01;
+----------+
| ?column? |
+----------+
| 32.0     |
+----------+
SELECT 2^3 AS RESULT;
+--------+
| result |
+--------+
| 8.0    |
+--------+
drop table if exists data_01;
SQL SUCCESS
