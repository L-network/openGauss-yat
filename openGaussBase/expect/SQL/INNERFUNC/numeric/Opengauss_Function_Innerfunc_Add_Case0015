-- @testpoint: 数字操作符+，多个数连续相加
drop table if exists data_01;
SQL SUCCESS
create table data_01 (clo1 int,clo2 int);
SQL SUCCESS
insert into data_01 values (255, 3);
SQL SUCCESS
select clo1 + clo2+12.555 from data_01;
+----------+
| ?column? |
+----------+
| 270.555  |
+----------+
SELECT 10 + 3+0.5 AS RESULT;
+--------+
| result |
+--------+
| 13.5   |
+--------+
drop table if exists data_01;
SQL SUCCESS
