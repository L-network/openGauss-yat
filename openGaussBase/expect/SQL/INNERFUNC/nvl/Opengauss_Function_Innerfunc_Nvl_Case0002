-- @testpoint: nvl函数参数1不为空，参数2为null
select nvl(1,null);
+-----+
| nvl |
+-----+
| 1   |
+-----+
select nvl('string',null);
+--------+
| nvl    |
+--------+
| string |
+--------+
select length(nvl(to_date('2018','yyyy'),null));
+--------+
| length |
+--------+
| 19     |
+--------+
select nvl(1.1,null);
+-----+
| nvl |
+-----+
| 1.1 |
+-----+
