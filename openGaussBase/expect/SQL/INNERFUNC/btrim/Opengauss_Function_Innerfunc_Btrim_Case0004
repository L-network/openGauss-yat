-- @testpoint: 参数区分大小写
SELECT btrim('hellowusdgeuihello34678sadhjhello' , 'HELLO');
+-----------------------------------+
| btrim                             |
+-----------------------------------+
| hellowusdgeuihello34678sadhjhello |
+-----------------------------------+
SELECT btrim('hellowusdgeuihello34678sadhjhello' , 'hello');
+-------------------------+
| btrim                   |
+-------------------------+
| wusdgeuihello34678sadhj |
+-------------------------+
