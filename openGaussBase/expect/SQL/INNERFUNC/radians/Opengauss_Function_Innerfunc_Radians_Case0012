-- @testpoint: radians函数函数嵌套测试
select radians(radians(1000));
+--------------------+
| radians            |
+--------------------+
| 0.3046174197867086 |
+--------------------+
select radians(radians('1000'));
+--------------------+
| radians            |
+--------------------+
| 0.3046174197867086 |
+--------------------+
