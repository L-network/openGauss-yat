-- @testpoint: rpad函数入参为数值函数
select rpad(abs(-3435354356),34,'sdsds') from sys_dummy;
+------------------------------------+
| rpad                               |
+------------------------------------+
| 3435354356sdsdssdsdssdsdssdsdssdsd |
+------------------------------------+
select rpad(acos(-0.9),34,'sdsds') from sys_dummy;
+------------------------------------+
| rpad                               |
+------------------------------------+
| 2.69056584179353075sdsdssdsdssdsds |
+------------------------------------+
