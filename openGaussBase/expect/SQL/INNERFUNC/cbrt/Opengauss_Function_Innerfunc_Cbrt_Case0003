

--  @testpoint:函数嵌套使用

select cbrt(cbrt(27));
+--------------------+
| cbrt               |
+--------------------+
+--------------------+
select cbrt(power(2.3,3));
+--------------------+
| cbrt               |
+--------------------+
+--------------------+
select cbrt(pi());
+--------------------+
| cbrt               |
+--------------------+
+--------------------+
