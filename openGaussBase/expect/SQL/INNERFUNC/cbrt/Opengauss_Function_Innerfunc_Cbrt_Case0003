

--  @testpoint:函数嵌套使用

select cbrt(cbrt(27));
+--------------------+
| cbrt               |
+--------------------+
| 1.4422495703074085 |
+--------------------+
select cbrt(power(2.3,3));
+--------------------+
| cbrt               |
+--------------------+
+--------------------+
select cbrt(pi());
+--------------------+
| cbrt               |
+--------------------+
| 1.4645918875615231 |
+--------------------+
