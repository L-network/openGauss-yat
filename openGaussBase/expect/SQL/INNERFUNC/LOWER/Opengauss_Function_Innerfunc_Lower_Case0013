-- @testpoint: lower函数作为count的入参
drop table if exists TEST_LOWER_006;
SQL SUCCESS
create table TEST_LOWER_006 (COL1 varchar2(20));
SQL SUCCESS
insert into TEST_LOWER_006 values('*');
SQL SUCCESS
select count(lower(COL1)) from TEST_LOWER_006 order by 1;
+-------+
| count |
+-------+
| 1     |
+-------+
drop table if exists TEST_LOWER_006;
SQL SUCCESS
