-- @testpoint:验证cast函数是否支持索引
DROP TABLE IF EXISTS TEST1;
SQL SUCCESS
DROP INDEX IF EXISTS TEST1_INDEX;
SQL SUCCESS
CREATE TABLE TEST1(RIQI INT);
SQL SUCCESS
CREATE INDEX TEST1_INDEX ON TEST1(RIQI);
SQL SUCCESS
SELECT * FROM TEST1 WHERE RIQI<CAST('2018-08-30' AS DATE);
+------+
| riqi |
+------+
+------+
DROP INDEX IF EXISTS TEST1_INDEX;
SQL SUCCESS
DROP TABLE IF EXISTS TEST1;
SQL SUCCESS
