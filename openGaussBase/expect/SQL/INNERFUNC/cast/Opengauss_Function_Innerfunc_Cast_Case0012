-- @testpoint: 验证cast函数是否支持伪列
-- @modify at: 2020-11-16
drop table if exists TEST2;
SQL SUCCESS
create table TEST2 (RIQI int);
SQL SUCCESS
insert into TEST2 values(1),(2),(9);
SQL SUCCESS
select ROWNUM from TEST2;
+--------+
| rownum |
+--------+
| 1      |
| 2      |
| 3      |
+--------+
drop table if exists TEST2;
SQL SUCCESS
