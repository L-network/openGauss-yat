--查询
select t1.c_integer,t1.c_real,t2.c_integer c1 ,t2.c_real c2 from zsharding_tbl t1 full join zsharding_tbl t2 on t1.c_integer=t2.c_real order by 1,2,3,4;
+-------------+---------------+-------------+---------------+
| c_integer   | c_real        | c1          | c2            |
