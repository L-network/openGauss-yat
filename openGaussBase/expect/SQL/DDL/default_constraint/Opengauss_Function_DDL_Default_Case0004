-- @testpoint: 增加字段用默认值，先not null后default
DROP TABLE IF EXISTS META_ENUM;
SQL SUCCESS
CREATE TABLE IF NOT EXISTS META_ENUM 
(ID BIGINT NOT NULL,
 ATTR_ID BIGINT NULL,
 NAME VARCHAR(60) NOT NULL,
   VALUE_LIST VARCHAR(1000),
   I18N_LIST VARCHAR(2000),
   PRIMARY KEY (ID))
;
SQL SUCCESS
ALTER TABLE META_ENUM ADD COLUMN location3 VARCHAR(255) NOT NULL DEFAULT 'common';
SQL SUCCESS
DROP TABLE META_ENUM;
SQL SUCCESS
