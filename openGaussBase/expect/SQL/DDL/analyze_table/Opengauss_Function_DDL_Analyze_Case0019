-- @testpoint:  多次统计分析表
drop table if exists t1;
SQL SUCCESS
create table t1(a int);
SQL SUCCESS
insert into t1 values(1);
SQL SUCCESS
insert into t1 values(2);
SQL SUCCESS
insert into t1 values(3);
SQL SUCCESS
insert into t1 values(4);
SQL SUCCESS
analyze  t1 ;
SQL SUCCESS
delete from t1;
SQL SUCCESS
insert into t1 values(5);
SQL SUCCESS
analyze  t1;
SQL SUCCESS
drop table if exists t1;
SQL SUCCESS
