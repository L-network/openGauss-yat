-- @testpoint: 约束名为64位（opengauss中没有明确长度限制，所以现在超出长度会截断并不会报错）
DROP TABLE IF EXISTS tal_09;
SQL SUCCESS
CREATE TABLE tal_09( C_CHAR20 CHAR(20) not null, C_INT INT);
SQL SUCCESS
ALTER TABLE tal_09 ADD CONSTRAINT BAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAABBBB64 UNIQUE(C_INT);
SQL SUCCESS
INSERT INTO tal_09 VALUES('2020-07-06',53);
SQL SUCCESS
INSERT INTO tal_09 VALUES('2020-07-06',369);
SQL SUCCESS
ALTER TABLE tal_09 DROP CONSTRAINT  IF EXISTS BAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAABBBB64 CASCADE ;
SQL SUCCESS
DROP TABLE IF EXISTS tal_09;
SQL SUCCESS
