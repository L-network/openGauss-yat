-- @testpoint: 约束名为63位
DROP TABLE IF EXISTS tal_01;
SQL SUCCESS
CREATE TABLE tal_01( C_CHAR20 CHAR(20) not null, C_INT INT);
SQL SUCCESS
ALTER TABLE tal_01 ADD CONSTRAINT BAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAABBBB63 UNIQUE(C_INT);
SQL SUCCESS
INSERT INTO tal_01 VALUES('2020-07-06',53);
SQL SUCCESS
INSERT INTO tal_01 VALUES('2020-07-06',369);
SQL SUCCESS
ALTER TABLE tal_01 DROP CONSTRAINT  IF EXISTS BAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAABBBB63 CASCADE ;
SQL SUCCESS
DROP TABLE IF EXISTS tal_01;
SQL SUCCESS
