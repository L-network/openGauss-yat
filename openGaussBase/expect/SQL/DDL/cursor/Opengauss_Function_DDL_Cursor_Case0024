--  @testpoint:cursor声明游标，使用value子句，子句类型为几何类型；
start transaction;
SQL SUCCESS
cursor cursor24 for values(box(circle '((0,0),2.0)'),diameter(circle '((0,0),2.0)'));
SQL SUCCESS
fetch from cursor24;
+-------------------------------------------------------------------------------+---------+
| column1                                                                       | column2 |
+-------------------------------------------------------------------------------+---------+
| (1.414213562373095,1.414213562373095),(-1.414213562373095,-1.414213562373095) | 4.0     |
+-------------------------------------------------------------------------------+---------+
close cursor24;
SQL SUCCESS
end;
SQL SUCCESS
