-- @testpoint: create 列存表table 与 with 子句（COMPRESSION,）
DROP TABLE IF EXISTS tab_12;
SQL SUCCESS
CREATE TABLE tab_12
(id                      NUMBER(7),
 use_filename              VARCHAR2(20)
)with(ORIENTATION=COLUMN,COMPRESSION=YES);
SQL SUCCESS
drop table if exists tab_12;
SQL SUCCESS
DROP TABLE IF EXISTS tab_13;
SQL SUCCESS
CREATE TABLE tab_13
(id                      NUMBER(7),
 name              VARCHAR2(20)
  )with(ORIENTATION=COLUMN,COMPRESSION=NO);
SQL SUCCESS
DROP TABLE IF EXISTS tab_13;
SQL SUCCESS
DROP TABLE IF EXISTS tab_14;
SQL SUCCESS
CREATE TABLE tab_14
(id                      NUMBER(7),
 name              VARCHAR2(20)
  )with(ORIENTATION=COLUMN,COMPRESSION=MIDDLE);
SQL SUCCESS
DROP TABLE IF EXISTS tab_14;
SQL SUCCESS
DROP TABLE IF EXISTS tab_15;
SQL SUCCESS
CREATE TABLE tab_15
(id                      NUMBER(7),
 name              VARCHAR2(20)
  )with(ORIENTATION=COLUMN,COMPRESSION=HIGH);
SQL SUCCESS
DROP TABLE IF EXISTS tab_15;
SQL SUCCESS
