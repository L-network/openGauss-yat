-- @testpoint: 创建带空约束的表(列级)
DROP TABLE IF EXISTS tab_12;
SQL SUCCESS
CREATE TABLE tab_12
(id                      NUMBER(7) ,
 use_filename              VARCHAR2(20),
 filename                  VARCHAR2(255) NULL,
 text                       VARCHAR2(2000)
);
SQL SUCCESS
insert into tab_12 values(1,'李','小龙','截拳道大师');
SQL SUCCESS
drop table if exists tab_12;
SQL SUCCESS
