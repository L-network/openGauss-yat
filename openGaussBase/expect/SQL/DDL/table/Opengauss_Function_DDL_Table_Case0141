-- @testpoint: 创建列类型是整数类型bigint类型的表,超边界时合理报错
drop table if exists table_1;
SQL SUCCESS
create table table_1(a BIGINT);
SQL SUCCESS
SQL SUCCESS
SQL SUCCESS
SQL SUCCESS
?.*ERROR: bigint out of range
  Where: referenced column: a
?.*ERROR: bigint out of range
  Where: referenced column: a
select * from table_1;
+----------------------+
| a                    |
+----------------------+
+----------------------+
drop table if exists table_1;
SQL SUCCESS
