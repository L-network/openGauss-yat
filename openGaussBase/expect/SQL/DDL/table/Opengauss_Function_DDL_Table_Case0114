-- @testpoint: 创建列类型是NUMERIC[(p[,s])的表
drop table if exists NUMERIC_t1;
SQL SUCCESS
CREATE TABLE NUMERIC_t1
(
    DT DECIMAL(10,4)
);
SQL SUCCESS
INSERT INTO NUMERIC_t1 VALUES(123456.122331);
SQL SUCCESS
select * from  NUMERIC_t1;
+-------------+
| dt          |
+-------------+
| 123456.1223 |
+-------------+
drop table if exists NUMERIC_t1;
SQL SUCCESS
