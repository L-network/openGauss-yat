-- @testpoint: 创建列类型是BINARY_INTEGER的表，超出边界时合理报错
drop table if exists table_1;
SQL SUCCESS
create table table_1(a BINARY_INTEGER);
SQL SUCCESS
SQL SUCCESS
SQL SUCCESS
SQL SUCCESS
--ERROR:  integer out of range
?.*ERROR: integer out of range.*
  Where: referenced column: a
?.*ERROR: integer out of range.*
  Where: referenced column: a
select * from table_1;
+-------------+
| a           |
+-------------+
+-------------+
drop table if exists table_1;
SQL SUCCESS
