-- @testpoint: openGauss关键字path(非保留）,作为列名不带引号，使用时带双引号或反引号，大小写匹配,合理报错
drop table if exists path_test;
SQL SUCCESS
create table path_test(
	c_id int, c_int int, c_integer integer, c_bool int, c_boolean int, c_bigint integer,
	c_real real, c_double real,
	c_decimal decimal(38), c_number number(38), c_numeric numeric(38),
	c_char char(50) default null, c_varchar varchar(20), c_varchar2 varchar2(4000),
	c_date date, c_datetime date, c_timestamp timestamp,
	path char(50),
	path char(50)
)
PARTITION BY RANGE (c_integer)
(
	partition P_max values less than (maxvalue)
);
?.*ERROR: column "path" specified more than once.*
--插入时带双引号，插入数据成功
insert into path_test(c_id,"path") values(2,'china');
?.*ERROR: relation "path_test" does not exist.*
?.*Position: .*
--插入时带反引号，插入数据失败
insert into path_test(c_id,`path`) values(2,'china');
?.*ERROR: syntax error at or near "`".*
?.*Position: .*
select * from path_test;
?.*ERROR: relation "path_test" does not exist.*
?.*Position: .*
drop table path_test;
?.*ERROR: table "path_test" does not exist.*
