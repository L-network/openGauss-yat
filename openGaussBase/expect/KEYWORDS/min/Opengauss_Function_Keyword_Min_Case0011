--  @testpoint:openGauss关键字min(非保留)，同时作为表名和列名带引号，并进行dml操作,min列的值最终显示为1000
drop table if exists "min";
SQL SUCCESS
create table "min"(
	c_id int, c_int int, c_integer integer, c_bool int, c_boolean int, c_bigint integer,
	c_real real, c_double real,
	c_decimal decimal(38), c_number number(38), c_numeric numeric(38),
	c_char char(50) default null, c_varchar varchar(20), c_varchar2 varchar2(4000),
	c_date date, c_datetime date, c_timestamp timestamp,
	"min" varchar(100) default 'min'
)
PARTITION BY RANGE (c_integer)
(
	partition P_max values less than (maxvalue)
);
SQL SUCCESS
insert into "min"(c_id,"min") values(1,'hello');
SQL SUCCESS
insert into "min"(c_id,"min") values(2,'china');
SQL SUCCESS
update "min" set "min"=1000 where "min"='hello';
SQL SUCCESS
delete from "min" where "min"='china';
SQL SUCCESS
select "min" from "min" where "min"!='hello' order by "min";
+------+
| min  |
+------+
| 1000 |
+------+
drop table "min";
SQL SUCCESS
