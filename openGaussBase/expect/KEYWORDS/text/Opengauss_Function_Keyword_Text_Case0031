--  @testpoint:opengauss关键字text(非保留)，作为字段数据类型(合理报错)
--前置条件
drop table if exists explain_test cascade;
SQL SUCCESS
--关键字不带引号-合理报错
create table explain_test(id int,name text);
SQL SUCCESS
--关键字带双引号-合理报错
create table explain_test(id int,name "text");
?.*ERROR: relation "explain_test" already exists.*
--关键字带单引号-合理报错
create table explain_test(id int,name 'text');
?.*ERROR: syntax error at or near "'text'".*
?.*Position: .*
--关键字带反引号-合理报错
create table explain_test(id int,name `text`);
?.*ERROR: syntax error at or near "`".*
?.*Position: .*
