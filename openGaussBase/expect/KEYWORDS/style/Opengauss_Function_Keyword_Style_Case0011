--  @testpoint:openGauss关键字style(非保留)，同时作为表名和列名带引号，并进行dml操作,style列的值最终显示为1000
drop table if exists "style" CASCADE;
SQL SUCCESS
create table "style"(
	c_id int, c_int int, c_integer integer, c_bool int, c_boolean int, c_bigint integer,
	c_real real, c_double real,
	c_decimal decimal(38), c_number number(38), c_numeric numeric(38),
	c_char char(50) default null, c_varchar varchar(20), c_varchar2 varchar2(4000),
	c_date date, c_datetime date, c_timestamp timestamp,
	"style" varchar(100) default 'style'
)
PARTITION BY RANGE (c_integer)
(
	partition P_max values less than (maxvalue)
);
SQL SUCCESS
insert into "style"(c_id,"style") values(1,'hello');
SQL SUCCESS
insert into "style"(c_id,"style") values(2,'china');
SQL SUCCESS
update "style" set "style"=1000 where "style"='hello';
SQL SUCCESS
delete from "style" where "style"='china';
SQL SUCCESS
select "style" from "style" where "style"!='hello' order by "style";
+-------+
| style |
+-------+
| 1000  |
+-------+
drop table "style";
SQL SUCCESS
