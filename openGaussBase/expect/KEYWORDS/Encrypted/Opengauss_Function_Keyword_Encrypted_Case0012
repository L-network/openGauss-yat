--  @testpoint:openGauss关键字encrypted(非保留)同时作为表名和列名带引号,并使用该列结合limit排序,encrypted列的值按字母大小排序且只显示前2条数据
drop table if exists "encrypted";
SQL SUCCESS
create table "encrypted"(
	c_id int, c_int int, c_integer integer, c_bool int, c_boolean int, c_bigint integer,
	c_real real, c_double real,
	c_decimal decimal(38), c_number number(38), c_numeric numeric(38),
	c_char char(50) default null, c_varchar varchar(20), c_varchar2 varchar2(4000),
	c_date date, c_datetime date, c_timestamp timestamp,
	"encrypted" varchar(100) default 'encrypted'
)
PARTITION BY RANGE (c_integer)
(
	partition P_max values less than (maxvalue)
);
SQL SUCCESS
delete from "encrypted";
SQL SUCCESS
insert into "encrypted"(c_id,"encrypted") values(1,'hello');
SQL SUCCESS
insert into "encrypted"(c_id,"encrypted") values(2,'china');
SQL SUCCESS
insert into "encrypted"(c_id,"encrypted") values(3,'gauss');
SQL SUCCESS
select "encrypted" from "encrypted" where "encrypted"!='hello' order by "encrypted" limit 2 ;
+-----------+
| encrypted |
+-----------+
| china     |
| gauss     |
+-----------+
drop table "encrypted";
SQL SUCCESS
