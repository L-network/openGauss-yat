--  @testpoint:openGauss关键字privilege(非保留)，同时作为表名和列名带引号，并进行dml操作,privilege列的值最终显示为1000
drop table if exists "privilege";
SQL SUCCESS
create table "privilege"(
	c_id int, c_int int, c_integer integer, c_bool int, c_boolean int, c_bigint integer,
	c_real real, c_double real,
	c_decimal decimal(38), c_number number(38), c_numeric numeric(38),
	c_char char(50) default null, c_varchar varchar(20), c_varchar2 varchar2(4000),
	c_date date, c_datetime date, c_timestamp timestamp,
	"privilege" varchar(100) default 'privilege'
)
PARTITION BY RANGE (c_integer)
(
	partition P_max values less than (maxvalue)
);
SQL SUCCESS
insert into "privilege"(c_id,"privilege") values(1,'hello');
SQL SUCCESS
insert into "privilege"(c_id,"privilege") values(2,'china');
SQL SUCCESS
update "privilege" set "privilege"=1000 where "privilege"='hello';
SQL SUCCESS
delete from "privilege" where "privilege"='china';
SQL SUCCESS
select "privilege" from "privilege" where "privilege"!='hello' order by "privilege";
+-----------+
| privilege |
+-----------+
| 1000      |
+-----------+
drop table "privilege";
SQL SUCCESS
