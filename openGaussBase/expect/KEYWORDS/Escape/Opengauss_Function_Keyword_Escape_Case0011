--  @testpoint:openGauss关键字escape(非保留)，同时作为表名和列名带引号，并进行dml操作,escape列的值最终显示为1000
drop table if exists "escape";
SQL SUCCESS
create table "escape"(
	c_id int, c_int int, c_integer integer, c_bool int, c_boolean int, c_bigint integer,
	c_real real, c_double real,
	c_decimal decimal(38), c_number number(38), c_numeric numeric(38),
	c_char char(50) default null, c_varchar varchar(20), c_varchar2 varchar2(4000),
	c_date date, c_datetime date, c_timestamp timestamp,
	"escape" varchar(100) default 'escape'
)
PARTITION BY RANGE (c_integer)
(
	partition P_max values less than (maxvalue)
);
SQL SUCCESS
insert into "escape"(c_id,"escape") values(1,'hello');
SQL SUCCESS
insert into "escape"(c_id,"escape") values(2,'china');
SQL SUCCESS
update "escape" set "escape"=1000 where "escape"='hello';
SQL SUCCESS
delete from "escape" where "escape"='china';
SQL SUCCESS
select "escape" from "escape" where "escape"!='hello' order by "escape";
+--------+
| escape |
+--------+
| 1000   |
+--------+
drop table "escape";
SQL SUCCESS
