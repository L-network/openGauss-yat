--  @testpoint:openGauss关键字validation(非保留)，同时作为表名和列名带引号，并进行dml操作,validation列的值最终显示为1000
drop table if exists "validation" CASCADE;
SQL SUCCESS
create table "validation"(
	c_id int, c_int int, c_integer integer, c_bool int, c_boolean int, c_bigint integer,
	c_real real, c_double real,
	c_decimal decimal(38), c_number number(38), c_numeric numeric(38),
	c_char char(50) default null, c_varchar varchar(20), c_varchar2 varchar2(4000),
	c_date date, c_datetime date, c_timestamp timestamp,
	"validation" varchar(100) default 'validation'
)
PARTITION BY RANGE (c_integer)
(
	partition P_max values less than (maxvalue)
);
SQL SUCCESS
insert into "validation"(c_id,"validation") values(1,'hello');
SQL SUCCESS
insert into "validation"(c_id,"validation") values(2,'china');
SQL SUCCESS
update "validation" set "validation"=1000 where "validation"='hello';
SQL SUCCESS
delete from "validation" where "validation"='china';
SQL SUCCESS
select "validation" from "validation" where "validation"!='hello' order by "validation";
+------------+
| validation |
+------------+
| 1000       |
+------------+
drop table "validation";
SQL SUCCESS
