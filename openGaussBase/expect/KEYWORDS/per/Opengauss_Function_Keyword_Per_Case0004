-- @testpoint: openGauss关键字per(非保留）,作为列名不带引号，使用时带双引号或反引号，大小写匹配,合理报错
drop table if exists per_test;
SQL SUCCESS
create table per_test(
	c_id int, c_int int, c_integer integer, c_bool int, c_boolean int, c_bigint integer,
	c_real real, c_double real,
	c_decimal decimal(38), c_number number(38), c_numeric numeric(38),
	c_char char(50) default null, c_varchar varchar(20), c_varchar2 varchar2(4000),
	c_date date, c_datetime date, c_timestamp timestamp,
	per char(50),
	per char(50)
)
PARTITION BY RANGE (c_integer)
(
	partition P_max values less than (maxvalue)
);
?.*ERROR: column "per" specified more than once.*
--插入时带双引号，插入数据成功
insert into per_test(c_id,"per") values(2,'china');
?.*ERROR: relation "per_test" does not exist.*
?.*Position: .*
--插入时带反引号，插入数据失败
insert into per_test(c_id,`per`) values(2,'china');
?.*ERROR: syntax error at or near "`".*
?.*Position: .*
select * from per_test;
?.*ERROR: relation "per_test" does not exist.*
?.*Position: .*
drop table per_test;
?.*ERROR: table "per_test" does not exist.*
