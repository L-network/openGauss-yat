--  @testpoint:openGauss关键字optimization(非保留)，同时作为表名和列名带引号，并进行dml操作,optimization列的值最终显示为1000
drop table if exists "optimization";
SQL SUCCESS
create table "optimization"(
	c_id int, c_int int, c_integer integer, c_bool int, c_boolean int, c_bigint integer,
	c_real real, c_double real,
	c_decimal decimal(38), c_number number(38), c_numeric numeric(38),
	c_char char(50) default null, c_varchar varchar(20), c_varchar2 varchar2(4000),
	c_date date, c_datetime date, c_timestamp timestamp,
	"optimization" varchar(100) default 'optimization'
)
PARTITION BY RANGE (c_integer)
(
	partition P_max values less than (maxvalue)
);
SQL SUCCESS
insert into "optimization"(c_id,"optimization") values(1,'hello');
SQL SUCCESS
insert into "optimization"(c_id,"optimization") values(2,'china');
SQL SUCCESS
update "optimization" set "optimization"=1000 where "optimization"='hello';
SQL SUCCESS
delete from "optimization" where "optimization"='china';
SQL SUCCESS
select "optimization" from "optimization" where "optimization"!='hello' order by "optimization";
+--------------+
| optimization |
+--------------+
| 1000         |
+--------------+
drop table "optimization";
SQL SUCCESS
