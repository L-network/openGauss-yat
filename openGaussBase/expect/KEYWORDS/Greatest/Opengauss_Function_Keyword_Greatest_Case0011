--  @testpoint:openGauss关键字Greatest(非保留)，同时作为表名和列名带引号，并进行dml操作,Greatest列的值最终显示为1000
drop table if exists "Greatest";
SQL SUCCESS
create table "Greatest"(
	c_id int, c_int int, c_integer integer, c_bool int, c_boolean int, c_bigint integer,
	c_real real, c_double real,
	c_decimal decimal(38), c_number number(38), c_numeric numeric(38),
	c_char char(50) default null, c_varchar varchar(20), c_varchar2 varchar2(4000),
	c_date date, c_datetime date, c_timestamp timestamp,
	"Greatest" varchar(100) default 'Greatest'
)
PARTITION BY RANGE (c_integer)
(
	partition P_max values less than (maxvalue)
);
SQL SUCCESS
insert into "Greatest"(c_id,"Greatest") values(1,'hello');
SQL SUCCESS
insert into "Greatest"(c_id,"Greatest") values(2,'china');
SQL SUCCESS
update "Greatest" set "Greatest"=1000 where "Greatest"='hello';
SQL SUCCESS
delete from "Greatest" where "Greatest"='china';
SQL SUCCESS
select "Greatest" from "Greatest" where "Greatest"!='hello' order by "Greatest";
+----------+
| Greatest |
+----------+
| 1000     |
+----------+
drop table "Greatest";
SQL SUCCESS
