--  @testpoint:openGauss关键字position(非保留)同时作为表名和列名带引号,并使用该列结合limit排序,position列的值按字母大小排序且只显示前2条数据
drop table if exists "position";
SQL SUCCESS
create table "position"(
	c_id int, c_int int, c_integer integer, c_bool int, c_boolean int, c_bigint integer,
	c_real real, c_double real,
	c_decimal decimal(38), c_number number(38), c_numeric numeric(38),
	c_char char(50) default null, c_varchar varchar(20), c_varchar2 varchar2(4000),
	c_date date, c_datetime date, c_timestamp timestamp,
	"position" varchar(100) default 'position'
)
PARTITION BY RANGE (c_integer)
(
	partition P_max values less than (maxvalue)
);
SQL SUCCESS
delete from "position";
SQL SUCCESS
insert into "position"(c_id,"position") values(1,'hello');
SQL SUCCESS
insert into "position"(c_id,"position") values(2,'china');
SQL SUCCESS
insert into "position"(c_id,"position") values(3,'gauss');
SQL SUCCESS
select "position" from "position" where "position"!='hello' order by "position" limit 2 ;
+----------+
| position |
+----------+
| china    |
| gauss    |
+----------+
drop table "position";
SQL SUCCESS
