--  @testpoint:openGauss关键字condition_number(非保留)，作为列名带引号并且更新时使用该列，建表成功，condition_number的值更新为100
drop table if exists condition_number_test;
SQL SUCCESS
create table condition_number_test(
	c_id int, c_int int, c_integer integer, c_bool int, c_boolean int, c_bigint integer, 
	c_real real, c_double real, 
	c_decimal decimal(38), c_number number(38), c_numeric numeric(38), 
	c_char char(50) default null, c_varchar varchar(20), c_varchar2 varchar2(4000),
	c_date date, c_datetime date, c_timestamp timestamp,
	"condition_number" varchar(100) default 'condition_number'
)
PARTITION BY RANGE (c_integer)
(
	partition P_max values less than (maxvalue)
);
SQL SUCCESS
insert into condition_number_test(c_id,"condition_number") values(1,'hello');
SQL SUCCESS
update condition_number_test set "condition_number"=100;
SQL SUCCESS
select * from condition_number_test order by "condition_number";
+------+-------+-----------+--------+-----------+----------+--------+----------+-----------+----------+-----------+--------+-----------+------------+--------+------------+-------------+------------------+
| c_id | c_int | c_integer | c_bool | c_boolean | c_bigint | c_real | c_double | c_decimal | c_number | c_numeric | c_char | c_varchar | c_varchar2 | c_date | c_datetime | c_timestamp | condition_number |
+------+-------+-----------+--------+-----------+----------+--------+----------+-----------+----------+-----------+--------+-----------+------------+--------+------------+-------------+------------------+
| 1    |       |           |        |           |          |        |          |           |          |           |        |           |            |        |            |             | 100              |
+------+-------+-----------+--------+-----------+----------+--------+----------+-----------+----------+-----------+--------+-----------+------------+--------+------------+-------------+------------------+
drop table condition_number_test;
SQL SUCCESS
