-- @testpoint: opengauss关键字over(非保留)，自定义数据类型名为explain，部分测试点合理报错
drop table if exists score;
SQL SUCCESS
create table score(
  student_id varchar(10),
  course_id varchar(10),
  score decimal(18,1)
);
SQL SUCCESS
