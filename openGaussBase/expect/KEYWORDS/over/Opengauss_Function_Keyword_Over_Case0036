SELECT course_id, score,
RANK() OVER(ORDER BY score DESC)
FROM score;
+-----------+-------+------+
| course_id | score | rank |
+-----------+-------+------+
| 03        | 99.0  | 1    |
| 03        | 98.0  | 2    |
| 02        | 90.0  | 3    |
| 02        | 89.0  | 4    |
