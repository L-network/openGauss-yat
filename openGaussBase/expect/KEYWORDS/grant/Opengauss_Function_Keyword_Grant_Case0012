--  @testpoint:openGauss保留关键字grant同时作为表名和列名带引号,并使用该列结合limit排序,grant列的值按字母大小排序且只显示前2条数据
drop table if exists "grant";
SQL SUCCESS
create table "grant"(
	c_id int, c_int int, c_integer integer, c_bool int, c_boolean int, c_bigint integer,
	c_real real, c_double real,
	c_decimal decimal(38), c_number number(38), c_numeric numeric(38),
	c_char char(50) default null, c_varchar varchar(20), c_varchar2 varchar2(4000),
	c_date date, c_datetime date, c_timestamp timestamp,
	"grant" varchar(100) default 'grant'
)
PARTITION BY RANGE (c_integer)
(
	partition P_max values less than (maxvalue)
);
SQL SUCCESS
--清理表数据
delete from "grant";
SQL SUCCESS
--插入数据
insert into "grant"(c_id,"grant") values(1,'hello');
SQL SUCCESS
insert into "grant"(c_id,"grant") values(2,'china');
SQL SUCCESS
insert into "grant"(c_id,"grant") values(3,'gauss');
SQL SUCCESS
--查看表数据
select "grant" from "grant" where "grant"!='hello' order by "grant" limit 2 ;
+-------+
| grant |
+-------+
| china |
| gauss |
+-------+
--清理环境
drop table "grant";
SQL SUCCESS
