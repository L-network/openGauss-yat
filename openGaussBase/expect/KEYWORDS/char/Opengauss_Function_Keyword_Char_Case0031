--  @testpoint:opengauss关键字char(非保留)，作为字段数据类型(合理报错)
--前置条件
drop table if exists char_test cascade;
SQL SUCCESS
--关键字不带引号-合理报错
create table char_test(id int,name char);
SQL SUCCESS
--关键字带双引号-合理报错
create table char_test(id int,name "char");
?.*ERROR: relation "char_test" already exists.*
--关键字带单引号-合理报错
create table char_test(id int,name 'char');
?.*ERROR: syntax error at or near "'char'".*
?.*Position: .*
--关键字带反引号-合理报错
create table char_test(id int,name `char`);
?.*ERROR: syntax error at or near "`".*
?.*Position: .*
