--  @testpoint:openGauss关键字account(非保留)，作为列名带引号并且删除时使用该列,建表成功，account列值是'hello'的删除成功
--创建表
drop table if exists account_test;
SQL SUCCESS
create table account_test(
	c_id int, c_int int, c_integer integer, c_bool int, c_boolean int, c_bigint integer, 
	c_real real, c_double real, 
	c_decimal decimal(38), c_number number(38), c_numeric numeric(38), 
	c_char char(50) default null, c_varchar varchar(20), c_varchar2 varchar2(4000),
	c_date date, c_datetime date, c_timestamp timestamp,
	"account" varchar(100) default 'account'
) 
PARTITION BY RANGE (c_integer)
(
	partition P_max values less than (maxvalue)
);
SQL SUCCESS
--向表中插入数据
insert into account_test(c_id,"account") values(1,'hello');
SQL SUCCESS
--查询表内容
select * from account_test;
+------+-------+-----------+--------+-----------+----------+--------+----------+-----------+----------+-----------+--------+-----------+------------+--------+------------+-------------+---------+
| c_id | c_int | c_integer | c_bool | c_boolean | c_bigint | c_real | c_double | c_decimal | c_number | c_numeric | c_char | c_varchar | c_varchar2 | c_date | c_datetime | c_timestamp | account |
+------+-------+-----------+--------+-----------+----------+--------+----------+-----------+----------+-----------+--------+-----------+------------+--------+------------+-------------+---------+
| 1    |       |           |        |           |          |        |          |           |          |           |        |           |            |        |            |             | hello   |
+------+-------+-----------+--------+-----------+----------+--------+----------+-----------+----------+-----------+--------+-----------+------------+--------+------------+-------------+---------+
--向表中插入数据
insert into account_test(c_id) values(2);
SQL SUCCESS
--删除表数据
delete from account_test where "account"='hello';
SQL SUCCESS
--查询表内容
select * from account_test order by "account";
+------+-------+-----------+--------+-----------+----------+--------+----------+-----------+----------+-----------+--------+-----------+------------+--------+------------+-------------+---------+
| c_id | c_int | c_integer | c_bool | c_boolean | c_bigint | c_real | c_double | c_decimal | c_number | c_numeric | c_char | c_varchar | c_varchar2 | c_date | c_datetime | c_timestamp | account |
+------+-------+-----------+--------+-----------+----------+--------+----------+-----------+----------+-----------+--------+-----------+------------+--------+------------+-------------+---------+
| 2    |       |           |        |           |          |        |          |           |          |           |        |           |            |        |            |             | account |
+------+-------+-----------+--------+-----------+----------+--------+----------+-----------+----------+-----------+--------+-----------+------------+--------+------------+-------------+---------+
--清理环境
drop table account_test;
SQL SUCCESS
