--  @testpoint:openGauss保留关键字current_schema同时作为表名和列名带引号，并进行dml操作,current_schema列的值最终显示为1000
drop table if exists "current_schema";
SQL SUCCESS
create table "current_schema"(
	c_id int, c_int int, c_integer integer, c_bool int, c_boolean int, c_bigint integer,
	c_real real, c_double real,
	c_decimal decimal(38), c_number number(38), c_numeric numeric(38),
	c_char char(50) default null, c_varchar varchar(20), c_varchar2 varchar2(4000),
	c_date date, c_datetime date, c_timestamp timestamp,
	"current_schema" varchar(100) default 'current_schema'
)
PARTITION BY RANGE (c_integer)
(
	partition P_max values less than (maxvalue)
);
SQL SUCCESS
--插入数据
insert into "current_schema"(c_id,"current_schema") values(1,'hello');
SQL SUCCESS
insert into "current_schema"(c_id,"current_schema") values(2,'china');
SQL SUCCESS
--更新表数据
update "current_schema" set "current_schema"=1000 where "current_schema"='hello';
SQL SUCCESS
--清理表数据
delete from "current_schema" where "current_schema"='china';
SQL SUCCESS
--查看表数据
select "current_schema" from "current_schema" where "current_schema"!='hello' order by "current_schema";
+----------------+
| current_schema |
+----------------+
| 1000           |
+----------------+
--清理环境
drop table "current_schema";
SQL SUCCESS
