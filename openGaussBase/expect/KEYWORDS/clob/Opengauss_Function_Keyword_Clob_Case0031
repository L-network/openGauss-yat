--  @testpoint:opengauss关键字clob(非保留)，作为字段数据类型(合理报错)
--前置条件
drop table if exists clob_test cascade;
SQL SUCCESS
--关键字不带引号-合理报错
create table clob_test(id int,name clob);
SQL SUCCESS
--关键字带双引号-合理报错
create table clob_test(id int,name "clob");
?.*ERROR: relation "clob_test" already exists.*
--关键字带单引号-合理报错
create table clob_test(id int,name 'clob');
?.*ERROR: syntax error at or near "'clob'".*
?.*Position: .*
--关键字带反引号-合理报错
create table clob_test(id int,name `clob`);
?.*ERROR: syntax error at or near "`".*
?.*Position: .*
