--  @testpoint:openGauss保留关键字group同时作为表名和列名带引号,并使用该列结合limit排序,group列的值按字母大小排序且只显示前2条数据
drop table if exists "group";
SQL SUCCESS
create table "group"(
	c_id int, c_int int, c_integer integer, c_bool int, c_boolean int, c_bigint integer,
	c_real real, c_double real,
	c_decimal decimal(38), c_number number(38), c_numeric numeric(38),
	c_char char(50) default null, c_varchar varchar(20), c_varchar2 varchar2(4000),
	c_date date, c_datetime date, c_timestamp timestamp,
	"group" varchar(100) default 'group'
)
PARTITION BY RANGE (c_integer)
(
	partition P_max values less than (maxvalue)
);
SQL SUCCESS
--清理表数据
delete from "group";
SQL SUCCESS
--插入数据
insert into "group"(c_id,"group") values(1,'hello');
SQL SUCCESS
insert into "group"(c_id,"group") values(2,'china');
SQL SUCCESS
insert into "group"(c_id,"group") values(3,'gauss');
SQL SUCCESS
--查看表数据
select "group" from "group" where "group"!='hello' order by "group" limit 2 ;
+-------+
| group |
+-------+
| china |
| gauss |
+-------+
--清理环境
drop table "group";
SQL SUCCESS
