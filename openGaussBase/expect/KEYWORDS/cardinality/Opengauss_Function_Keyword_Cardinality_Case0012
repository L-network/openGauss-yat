--  @testpoint:openGauss关键字cardinality(非保留)同时作为表名和列名带引号,并使用该列结合limit排序,cardinality列的值按字母大小排序且只显示前2条数据
--创建表
drop table if exists "cardinality";
SQL SUCCESS
create table "cardinality"(
	c_id int, c_int int, c_integer integer, c_bool int, c_boolean int, c_bigint integer,
	c_real real, c_double real,
	c_decimal decimal(38), c_number number(38), c_numeric numeric(38),
	c_char char(50) default null, c_varchar varchar(20), c_varchar2 varchar2(4000),
	c_date date, c_datetime date, c_timestamp timestamp,
	"cardinality" varchar(100) default 'cardinality'
)
PARTITION BY RANGE (c_integer)
(
	partition P_max values less than (maxvalue)
);
SQL SUCCESS
--清除表数据
delete from "cardinality";
SQL SUCCESS
--向保重插入数据
insert into "cardinality"(c_id,"cardinality") values(1,'hello');
SQL SUCCESS
insert into "cardinality"(c_id,"cardinality") values(2,'china');
SQL SUCCESS
insert into "cardinality"(c_id,"cardinality") values(3,'gauss');
SQL SUCCESS
--查看表内容
select "cardinality" from "cardinality" where "cardinality"!='hello' order by "cardinality" limit 2 ;
+-------------+
| cardinality |
+-------------+
| china       |
| gauss       |
+-------------+
--清除环境
drop table "cardinality";
SQL SUCCESS
