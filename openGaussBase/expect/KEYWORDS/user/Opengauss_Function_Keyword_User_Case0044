-- @testpoint: 添加、修改、删除user列
drop table if exists user_05;
SQL SUCCESS
create table user_05 (name varchar(13),school varchar(13));
SQL SUCCESS
alter table user_05 add "user" char(10);
SQL SUCCESS
update user_05 set "user"='aa';
SQL SUCCESS
alter table user_05 drop "user";
SQL SUCCESS
drop table if exists user_05;
SQL SUCCESS