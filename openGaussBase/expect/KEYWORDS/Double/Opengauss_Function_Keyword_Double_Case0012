--  @testpoint:openGauss关键字double(非保留)同时作为表名和列名带引号,并使用该列结合limit排序,double列的值按字母大小排序且只显示前2条数据
drop table if exists "double";
SQL SUCCESS
create table "double"(
	c_id int, c_int int, c_integer integer, c_bool int, c_boolean int, c_bigint integer,
	c_real real, c_double real,
	c_decimal decimal(38), c_number number(38), c_numeric numeric(38),
	c_char char(50) default null, c_varchar varchar(20), c_varchar2 varchar2(4000),
	c_date date, c_datetime date, c_timestamp timestamp,
	"double" varchar(100) default 'double'
)
PARTITION BY RANGE (c_integer)
(
	partition P_max values less than (maxvalue)
);
SQL SUCCESS
delete from "double";
SQL SUCCESS
insert into "double"(c_id,"double") values(1,'hello');
SQL SUCCESS
insert into "double"(c_id,"double") values(2,'china');
SQL SUCCESS
insert into "double"(c_id,"double") values(3,'gauss');
SQL SUCCESS
select "double" from "double" where "double"!='hello' order by "double" limit 2 ;
+--------+
| double |
+--------+
| china  |
| gauss  |
+--------+
drop table "double";
SQL SUCCESS
