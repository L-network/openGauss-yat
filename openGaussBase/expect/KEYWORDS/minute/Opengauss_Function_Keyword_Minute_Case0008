--  @testpoint:openGauss关键字minute(非保留)，作为列名带引号并且删除时使用该列,建表成功，minute列值是'hello'的删除成功
drop table if exists explain_test;
SQL SUCCESS
create table explain_test(
	c_id int, c_int int, c_integer integer, c_bool int, c_boolean int, c_bigint integer, 
	c_real real, c_double real, 
	c_decimal decimal(38), c_number number(38), c_numeric numeric(38), 
	c_char char(50) default null, c_varchar varchar(20), c_varchar2 varchar2(4000),
	c_date date, c_datetime date, c_timestamp timestamp,
	"minute" varchar(100) default 'minute'
) 
PARTITION BY RANGE (c_integer)
(
	partition P_max values less than (maxvalue)
);
SQL SUCCESS
insert into explain_test(c_id,"minute") values(1,'hello');
SQL SUCCESS
insert into explain_test(c_id) values(2);
SQL SUCCESS
delete from explain_test where "minute"='hello';
SQL SUCCESS
select * from explain_test order by "minute";
+------+-------+-----------+--------+-----------+----------+--------+----------+-----------+----------+-----------+--------+-----------+------------+--------+------------+-------------+--------+
| c_id | c_int | c_integer | c_bool | c_boolean | c_bigint | c_real | c_double | c_decimal | c_number | c_numeric | c_char | c_varchar | c_varchar2 | c_date | c_datetime | c_timestamp | minute |
+------+-------+-----------+--------+-----------+----------+--------+----------+-----------+----------+-----------+--------+-----------+------------+--------+------------+-------------+--------+
| 2    |       |           |        |           |          |        |          |           |          |           |        |           |            |        |            |             | minute |
+------+-------+-----------+--------+-----------+----------+--------+----------+-----------+----------+-----------+--------+-----------+------------+--------+------------+-------------+--------+
drop table explain_test;
SQL SUCCESS
