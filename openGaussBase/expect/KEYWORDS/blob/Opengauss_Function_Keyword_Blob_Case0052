-- @testpoint: 修改字段类型为CHAR
DROP TABLE IF EXISTS t_blob;
SQL SUCCESS
CREATE TABLE t_blob(ID BLOB);
SQL SUCCESS
INSERT INTO t_blob VALUES('35466');
SQL SUCCESS
INSERT INTO t_blob VALUES('100000');
SQL SUCCESS
INSERT INTO t_blob VALUES('C4711A1097876CC');
SQL SUCCESS
INSERT INTO t_blob VALUES('');
SQL SUCCESS
INSERT INTO t_blob VALUES(NULL);
SQL SUCCESS
COMMIT;
SQL SUCCESS
ALTER TABLE T_BLOB MODIFY (ID CHAR(50));
SQL SUCCESS
ALTER TABLE T_BLOB MODIFY (ID VARCHAR(50));
SQL SUCCESS
UPDATE T_BLOB SET ID=NULL;
SQL SUCCESS
ALTER TABLE T_BLOB MODIFY (ID CHAR(50));
SQL SUCCESS
ALTER TABLE T_BLOB MODIFY (ID VARCHAR(50));
SQL SUCCESS
DELETE FROM T_BLOB;
SQL SUCCESS
ALTER TABLE T_BLOB MODIFY (ID CHAR(50));
SQL SUCCESS
ALTER TABLE T_BLOB MODIFY (ID VARCHAR(50));
SQL SUCCESS
drop table if exists t_blob;
SQL SUCCESS