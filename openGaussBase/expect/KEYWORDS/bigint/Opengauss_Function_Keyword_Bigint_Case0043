--  @testpoint:插入左边界范围值，未超出范围
drop table if exists bigint06;
SQL SUCCESS
create table bigint06 (name bigint);
SQL SUCCESS
insert into bigint06 values (-9223372036854775808 );
SQL SUCCESS
