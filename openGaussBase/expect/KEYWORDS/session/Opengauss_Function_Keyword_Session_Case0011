--  @testpoint:openGauss关键字session(非保留)，同时作为表名和列名带引号，并进行dml操作,session列的值最终显示为1000
drop table if exists "session" CASCADE;
SQL SUCCESS
create table "session"(
	c_id int, c_int int, c_integer integer, c_bool int, c_boolean int, c_bigint integer,
	c_real real, c_double real,
	c_decimal decimal(38), c_number number(38), c_numeric numeric(38),
	c_char char(50) default null, c_varchar varchar(20), c_varchar2 varchar2(4000),
	c_date date, c_datetime date, c_timestamp timestamp,
	"session" varchar(100) default 'session'
)
PARTITION BY RANGE (c_integer)
(
	partition P_max values less than (maxvalue)
);
SQL SUCCESS
insert into "session"(c_id,"session") values(1,'hello');
SQL SUCCESS
insert into "session"(c_id,"session") values(2,'china');
SQL SUCCESS
update "session" set "session"=1000 where "session"='hello';
SQL SUCCESS
delete from "session" where "session"='china';
SQL SUCCESS
select "session" from "session" where "session"!='hello' order by "session";
+---------+
| session |
+---------+
| 1000    |
+---------+
drop table "session";
SQL SUCCESS
