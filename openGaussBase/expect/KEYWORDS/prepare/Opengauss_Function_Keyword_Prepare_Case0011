--  @testpoint:openGauss关键字prepare(非保留)，同时作为表名和列名带引号，并进行dml操作,prepare列的值最终显示为1000
drop table if exists "prepare";
SQL SUCCESS
create table "prepare"(
	c_id int, c_int int, c_integer integer, c_bool int, c_boolean int, c_bigint integer,
	c_real real, c_double real,
	c_decimal decimal(38), c_number number(38), c_numeric numeric(38),
	c_char char(50) default null, c_varchar varchar(20), c_varchar2 varchar2(4000),
	c_date date, c_datetime date, c_timestamp timestamp,
	"prepare" varchar(100) default 'prepare'
)
PARTITION BY RANGE (c_integer)
(
	partition P_max values less than (maxvalue)
);
SQL SUCCESS
insert into "prepare"(c_id,"prepare") values(1,'hello');
SQL SUCCESS
insert into "prepare"(c_id,"prepare") values(2,'china');
SQL SUCCESS
update "prepare" set "prepare"=1000 where "prepare"='hello';
SQL SUCCESS
delete from "prepare" where "prepare"='china';
SQL SUCCESS
select "prepare" from "prepare" where "prepare"!='hello' order by "prepare";
+---------+
| prepare |
+---------+
| 1000    |
+---------+
drop table "prepare";
SQL SUCCESS
