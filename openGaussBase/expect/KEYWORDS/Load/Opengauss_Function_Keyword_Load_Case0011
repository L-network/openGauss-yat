--  @testpoint:openGauss关键字Load(非保留)，同时作为表名和列名带引号，并进行dml操作,Load列的值最终显示为1000
drop table if exists "Load";
SQL SUCCESS
create table "Load"(
	c_id int, c_int int, c_integer integer, c_bool int, c_boolean int, c_bigint integer,
	c_real real, c_double real,
	c_decimal decimal(38), c_number number(38), c_numeric numeric(38),
	c_char char(50) default null, c_varchar varchar(20), c_varchar2 varchar2(4000),
	c_date date, c_datetime date, c_timestamp timestamp,
	"Load" varchar(100) default 'Load'
)
PARTITION BY RANGE (c_integer)
(
	partition P_max values less than (maxvalue)
);
SQL SUCCESS
insert into "Load"(c_id,"Load") values(1,'hello');
SQL SUCCESS
insert into "Load"(c_id,"Load") values(2,'china');
SQL SUCCESS
update "Load" set "Load"=1000 where "Load"='hello';
SQL SUCCESS
delete from "Load" where "Load"='china';
SQL SUCCESS
select "Load" from "Load" where "Load"!='hello' order by "Load";
+------+
| Load |
+------+
| 1000 |
+------+
drop table "Load";
SQL SUCCESS
