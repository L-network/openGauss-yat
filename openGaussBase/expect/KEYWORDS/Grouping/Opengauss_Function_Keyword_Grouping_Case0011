--  @testpoint:openGauss关键字Grouping(非保留)，同时作为表名和列名带引号，并进行dml操作,Grouping列的值最终显示为1000
drop table if exists "Grouping";
SQL SUCCESS
create table "Grouping"(
	c_id int, c_int int, c_integer integer, c_bool int, c_boolean int, c_bigint integer,
	c_real real, c_double real,
	c_decimal decimal(38), c_number number(38), c_numeric numeric(38),
	c_char char(50) default null, c_varchar varchar(20), c_varchar2 varchar2(4000),
	c_date date, c_datetime date, c_timestamp timestamp,
	"Grouping" varchar(100) default 'Grouping'
)
PARTITION BY RANGE (c_integer)
(
	partition P_max values less than (maxvalue)
);
SQL SUCCESS
insert into "Grouping"(c_id,"Grouping") values(1,'hello');
SQL SUCCESS
insert into "Grouping"(c_id,"Grouping") values(2,'china');
SQL SUCCESS
update "Grouping" set "Grouping"=1000 where "Grouping"='hello';
SQL SUCCESS
delete from "Grouping" where "Grouping"='china';
SQL SUCCESS
select "Grouping" from "Grouping" where "Grouping"!='hello' order by "Grouping";
+----------+
| Grouping |
+----------+
| 1000     |
+----------+
drop table "Grouping";
SQL SUCCESS
