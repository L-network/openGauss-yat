--  @testpoint:openGauss关键字day(非保留)，同时作为表名和列名带引号，并进行dml操作,day列的值最终显示为1000
drop table if exists "day";
SQL SUCCESS
create table "day"(
	c_id int, c_int int, c_integer integer, c_bool int, c_boolean int, c_bigint integer,
	c_real real, c_double real,
	c_decimal decimal(38), c_number number(38), c_numeric numeric(38),
	c_char char(50) default null, c_varchar varchar(20), c_varchar2 varchar2(4000),
	c_date date, c_datetime date, c_timestamp timestamp,
	"day" varchar(100) default 'day'
)
PARTITION BY RANGE (c_integer)
(
	partition P_max values less than (maxvalue)
);
SQL SUCCESS
insert into "day"(c_id,"day") values(1,'hello');
SQL SUCCESS
insert into "day"(c_id,"day") values(2,'china');
SQL SUCCESS
update "day" set "day"=1000 where "day"='hello';
SQL SUCCESS
delete from "day" where "day"='china';
SQL SUCCESS
select "day" from "day" where "day"!='hello' order by "day";
+------+
| day  |
+------+
| 1000 |
+------+
drop table "day";
SQL SUCCESS
