--  @testpoint:openGauss关键字notify(非保留)，同时作为表名和列名带引号，并进行dml操作,notify列的值最终显示为1000
drop table if exists "notify";
SQL SUCCESS
create table "notify"(
	c_id int, c_int int, c_integer integer, c_bool int, c_boolean int, c_bigint integer,
	c_real real, c_double real,
	c_decimal decimal(38), c_number number(38), c_numeric numeric(38),
	c_char char(50) default null, c_varchar varchar(20), c_varchar2 varchar2(4000),
	c_date date, c_datetime date, c_timestamp timestamp,
	"notify" varchar(100) default 'notify'
)
PARTITION BY RANGE (c_integer)
(
	partition P_max values less than (maxvalue)
);
SQL SUCCESS
insert into "notify"(c_id,"notify") values(1,'hello');
SQL SUCCESS
insert into "notify"(c_id,"notify") values(2,'china');
SQL SUCCESS
update "notify" set "notify"=1000 where "notify"='hello';
SQL SUCCESS
delete from "notify" where "notify"='china';
SQL SUCCESS
select "notify" from "notify" where "notify"!='hello' order by "notify";
+--------+
| notify |
+--------+
| 1000   |
+--------+
drop table "notify";
SQL SUCCESS
