--  @testpoint:openGauss关键字end-exec(非保留)，作为列名带引号并且删除时使用该列,建表成功，end-exec列值是'hello'的删除成功
drop table if exists end-exec_test;
?.*ERROR: syntax error at or near "end"
?.*Position: .*
create table end-exec_test(
	c_id int, c_int int, c_integer integer, c_bool int, c_boolean int, c_bigint integer, 
	c_real real, c_double real, 
	c_decimal decimal(38), c_number number(38), c_numeric numeric(38), 
	c_char char(50) default null, c_varchar varchar(20), c_varchar2 varchar2(4000),
	c_date date, c_datetime date, c_timestamp timestamp,
	"end-exec" varchar(100) default 'end-exec'
) 
PARTITION BY RANGE (c_integer)
(
	partition P_max values less than (maxvalue)
);
?.*ERROR: syntax error at or near "end"
?.*Position: .*
insert into end-exec_test(c_id,"end-exec") values(1,'hello');
?.*ERROR: syntax error at or near "end"
?.*Position: .*
insert into end-exec_test(c_id) values(2);
?.*ERROR: syntax error at or near "end"
?.*Position: .*
delete from end-exec_test where "end-exec"='hello';
?.*ERROR: syntax error at or near "end"
?.*Position: .*
select * from end-exec_test order by "end-exec";
?.*ERROR: syntax error at or near "end"
?.*Position: .*
drop table end-exec_test;
?.*ERROR: syntax error at or near "end"
?.*Position: .*
